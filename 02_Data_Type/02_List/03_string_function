#We use third
vowel = [ "a","e","i","o","u"]
print(vowel)
print(vowel[2])

no = [1,2,3,4,5]
print(no)
print(no[3])

no = [2,6,9,7,5,8]
no.sort()
no.reverse()
print(no)
print(no[0:5])
print(no[:])
print(no[::2])
print(len(no))
print(min(no))

no.append(12)
print(no)

no.insert(1,50)
print(no)

no.remove(50)
print(no)


no.pop()
print(no)


print(no)
no[0] = 10
print(no)

#list is mutable means can change 







